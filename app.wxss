/* reset */
page {
  background-color: #F8F8F8;
  height: 100%;
  font-size: 32rpx;
  line-height: 1.6;
}
.radio{
  position: absolute;
  top:0;
  right:0;z-index: 5;
  display: block;
  width:100%;
  height:100%;
  margin:0;
}
button{
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}
form{
  width: 100%;
}
navigator:hover,navigator:active,navigator:focus{
  background:none; 
}