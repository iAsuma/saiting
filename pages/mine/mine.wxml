<view class="content">
  <view class="head">
    <block wx:if="{{islogined}}">
    <open-data class="tmp-img" type="userAvatarUrl"></open-data>
    <view class="h1"><open-data type="userNickName"></open-data></view>
    <view class="h5">账号：188888888888</view>
    </block>
    <block wx:else>
    <image src='/res/images/header1.png'></image>
    <button class="h1" open-type="getUserInfo" lang="zh_CN" bindtap="loginOrReg">登录/注册</button>
    </block>
  </view>
  <view class="list">
    <view class="item">
      <image src='/res/icons/c-1.png'></image>
      <text>我的订单</text>
      <view class="point"></view>
    </view>
    <view class="item">
      <image src='/res/icons/c-2.png'></image>
      <text>出行记录</text>
      <view class="point"></view>
    </view>
    <view class="item">
      <image src='/res/icons/c-3.png'></image>
      <text>停车券/红包</text>
      <view class="point"></view>
    </view>
    <view class="item">
      <image src='/res/icons/c-4.png'></image>
      <text>申请发票</text>
      <view class="point"></view>
    </view>
    <navigator class="item" url='../setting/setting'>
      <image src='/res/icons/c-5.png'></image>
      <text>账号设置</text>
      <view class="point"></view>
    </navigator>
  </view>
</view>

