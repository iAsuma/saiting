.padding-btm{
  float:left;
  width:100%;
  height:100rpx;
}
.car-list{
  float:left;
}
.car-list navigator{
  background:#fff;
  display: block;
  width:100%;
  border-radius: 10rpx;
  padding:20rpx;
  box-sizing: border-box;
  float:left;
}
.item image{
  width:160rpx;
  height:160rpx;
  border-radius: 10rpx;
  float:left;
  margin-bottom:20rpx;
}
.item-middle{
  width:calc(100% - 300rpx);
  margin-left:20rpx;
  float:left;
}
.h1{
  font-size: 32rpx;
  font-family: simHei;
}
.h4{
  font-size: 24rpx;
  color:#999;
  margin-bottom: 10rpx;
}
.nowrap{
  display: inline-block;
  width:100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.carPicker{
  width:100rpx;
  float:left;
  font-size: 30rpx;
  text-align: center;
}
.carIpt{
  width:calc(100% - 300rpx);
  float:left;
  height:100rpx;
  line-height:100rpx;
}
.item-right{
  float:right;
  text-align: center;
}
.price image{
  width:30rpx;
  height:30rpx;
  float:left;
  margin-top:10rpx;
}
.price{
  color:#E8373A;
  font-size: 32rpx;
  margin:30rpx 0 65rpx 0;
}
.dis{
  color:#999;
  font-size:26rpx;
}
.dis image{
  width:30rpx;
  height:30rpx;
  float:left;
  margin-top:5rpx;
}
.star{
  width:50%;
  float:left;
  font-size: 26rpx;
  color:#ff9900;
}
.star image{
  width:120rpx;
  height:20rpx;
  float:left;
  margin-top:10rpx;
  margin-right: 5rpx;
}
.count{
  width:50%;
  float:left;
  font-size: 26rpx;
  color:#999;
  text-align: center
}
.item-foot{
  width:100%;
  float:left;
  border-top:1rpx solid #eee;
  padding-top:20rpx;
}
.item-foot text{
  display: inline-block;
  background:#EBEEF7;
  font-size: 28rpx;
  padding:0 20rpx;
  border-radius: 30rpx;
  margin-right:20rpx;
}


/* page/component/pages/public/public.wxss */

.form{
  padding-top:0;
  float:left;
  width:100%;
  margin-top:20rpx;
}
.item-form{
  padding:0 30rpx;
  height:100rpx;
  line-height:100rpx;
  border-bottom:1rpx solid #eee;
  background: #fff;
}
.item-form text{
  float:left;
  width:200rpx;
  font-size: 30rpx;
}
.item-form image{
  float:right;
  width:40rpx;
  height:40rpx;
  margin-top:-70rpx;
}
.priceIpt{
  height:100rpx;
  line-height:100rpx;
  font-size: 28rpx;
  width:calc(100% - 250rpx);
}
.bor-top{
  margin-top:20rpx;
}

.item-txt{
  background:#fff;
  padding:30rpx;
  margin-top:20rpx;
  float:left;
  width:100%;
  box-sizing: border-box;
}
.item-txt>text{
  font-family: SimHei;
  font-size: 32rpx;
  margin-bottom:20rpx;
  display: block;
}
.contact image{
  border-radius: 50%;
  width:120rpx;
  height:120rpx;
  float:left;
}
.car-mes{
  float:left;
  margin-left:30rpx;
}
.contact .h2{
  font-family: SimHei;
  
}
.contact .h3{
  color:#999;
}

.btn-red-mini{
  border:0;
  color:#fff;
  background:#E83539;
  font-size: 30rpx;
  line-height:80rpx;
  height:80rpx;
  float:right;
}
.btn-red-mini image{
  width:50rpx;
  height:50rpx;
  float:left;
  margin:15rpx;
}
.btn-red{
  border:0;
  color:#fff;
  background:#E83539;
  font-size: 30rpx;
  line-height:80rpx;
  height:80rpx;
  margin-top:40rpx;
}
.btn-box{
  padding:30rpx;
  box-sizing: border-box;
}
.text-list text{
  font-size: 26rpx;
  color:#666;
  display: block;
}
.pro-list text{
  display: inline-block;
  padding:0 20rpx;
  margin-right:20rpx;
  font-size: 28rpx;
  background:#EBEEF7;
  border-radius: 30rpx;
}
.fiex-btn{
  position: fixed;
  bottom:0;
  left:0;
  width:100%;
  z-index: 5;
  background: #fff;
  border-top:1px solid #eee;
  height:100rpx;
  line-height:100rpx;
  padding-left:20rpx;
}
.fiex-btn .left{
  float:left;
  width:200rpx;
}
.fiex-btn .middle{
  float:left;
}
.fiex-btn image{
  width:30rpx;
  height:30rpx;
}
.fiex-btn text{
  color:#E8373A;
}
.btn-pay{
  width:300rpx;
  background:#E83539;
  color:#fff;
  border-radius: 0;
  margin:0;
  height:100rpx;
  line-height:100rpx;
  float:right;
}